<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="com.PersonalProject.style.dao.StyleMapper">
 	<select id="selectStyleByProductId" parameterType="int" resultType="com.PersonalProject.style.model.Style">
 		SELECT
 			`id`
			, `productId`
			, `userId`
			, `shoesImagePath`
			, `content`
			, `createdAt`
			, `updatedAt`
		FROM
			`style`
		WHERE
			`productId` = #{productId}
 	</select>
 </mapper>